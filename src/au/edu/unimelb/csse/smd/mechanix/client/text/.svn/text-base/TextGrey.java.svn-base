package au.edu.unimelb.csse.smd.mechanix.client.text;

import au.edu.unimelb.csse.smd.mechanix.client.util.text.ImageText;

public class TextGrey extends ImageText {

	public TextGrey(String text) {
		super(text, FontCollection.fontGrey());
	}

	public TextGrey(String text, float size) {
		this(text);
		setSize(size);
	}

}
