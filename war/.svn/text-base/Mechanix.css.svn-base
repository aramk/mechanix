/** Add css rules here for your application. */
	/** Example rules used by the template application (remove for your app) */
h1 {
	font-size: 2em;
	font-weight: bold;
	color: #777777;
	margin: 40px 0px 70px;
	text-align: center;
}

.sendButton {
	display: block;
	font-size: 16pt;
}

/** Most GWT widgets already have a style name defined */
.gwt-DialogBox {
	width: 400px;
}

.dialogVPanel {
	margin: 5px;
}

.serverResponseLabelError {
	color: red;
}

/** Set ids using widget.getElement().setId("idOfElement") */
#closeButton {
	margin: 15px 6px 6px;
}

body {
	background: #EEE !important;
}

.test {
	background: red;
}

.gwt-LevelWin,
.gwt-GameOver {
	width: 100%;
	text-align: center;
	z-Index: 1000;
}

.gwt-Menu,.gwt-Game {
	position: relative !important;
	margin: 0 auto;
}

.gwt-PushButton {
	border: 0 !important;
	background: none !important;
	outline: 0px;
}

.gwt-Stage,.gwt-Bench,.gwt-Button,.gwt-PushButton,img {
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-o-user-select: none;
	user-select: none;
}

.gwt-Stage {
	/*background: #CCC;*/
}

.gwt-Bench {
	/*background: yellow;*/
	overflow-x: auto;
	overflow-y: hidden !important;
}

.gwt-BenchPanel {
	height: 100px;
	margin: 0 auto;
}

.gwt-BenchPanel td {
	height: 90px;
	width: 90px;
}

.gwt-BenchPanel td img {
	/*border: 1px solid;*/
}

.gwt-ImageText img {
	margin: 0 -1.5px;
}

.gwt-ImageTextCenter,.gwt-ImageTextRight,.gwt-MenuPanel {
	width: 776px;
	padding: 0 12px;
}

.gwt-ImageTextCenter {
	text-align: center;
}

.gwt-ImageTextRight {
	text-align: right;
}

.gwt-Image-Repeat {
	background: #CCC url(data:image/jpeg;base64,
	);
	border: 1px solid #000;
	background-repeat: repeat;
}

.gwt-MenuPanel {
	text-align: center;
}

.gwt-AboutPanel {
	width: 100%;
	text-align: center;
}

.gwt-MenuPanel img {
	clear: both;
}

.dragdrop-draggable {
	z-index: 10;
}

.dragdrop-positioner {
	display: none !important;
}

.pointer {
	cursor: hand;
}